<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="hibernate._01.model.members.MembersBean" table="members">
        <id name="memberID" column="memberID">
        	<generator class="guid"/>
        </id>
        <property name="firstName" column="firstName"/>
        <property name="lastName" column="lastName"/>
        <property name="nickname" column="nickname"/>
        <property name="email" column="email"/>
        <property name="gender" column="gender"/>
        <property name="birthday" column="birthday"/>
        <property name="countryID" column="countryID"/>
        <property name="cityID" column="cityID"/>
        <property name="locationID" column="locationID"/>
        <property name="address" column="address"/>
        <property name="height" column="height"/>
        <property name="weight" column="weight"/>
        <property name="phone" column="phone"/>
        <property name="photo" column="photo" type="binary"/>
        <property name="competenceID" column="competenceID"/>
        <property name="identityID" column="identityID"/>
        <property name="emergencyContact" column="emergencyContact"/>
        <property name="emergencyPhone" column="emergencyPhone"/>
        <property name="emergencyRelation" column="emergencyRelation"/>
        <property name="createDate" column="createDate"/>
        <property name="lastOnlineDateTime" column="lastOnlineDateTime"/>
    </class>
</hibernate-mapping>